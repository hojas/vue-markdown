{
  "name": "vue-md-render",
  "version": "0.1.1",
  "packageManager": "pnpm@7.14.1",
  "license": "MIT",
  "keywords": [
    "remark",
    "unified",
    "markdown",
    "commonmark",
    "gfm",
    "ast",
    "vue",
    "vue-component",
    "component"
  ],
  "repository": "hojas/vue-md-render",
  "bugs": "https://github.com/hojas/vue-md-render/issues",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/vue-md-render.d.ts",
  "main": "./dist/vue-md-render.umd.cjs",
  "module": "./dist/vue-md-render.js",
  "exports": {
    ".": {
      "import": "./dist/vue-md-render.js",
      "require": "./dist/vue-md-render.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite --config vite.config.ts example",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "rehype-stringify": "^9.0.3",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/node": "^18.11.8",
    "@vitejs/plugin-vue": "^3.2.0",
    "github-markdown-css": "^5.1.0",
    "highlight.js": "^11.6.0",
    "katex": "^0.16.3",
    "mermaid": "^9.1.7",
    "prettier": "^2.7.1",
    "rehype-highlight": "^6.0.0",
    "rehype-katex": "^6.0.2",
    "rehype-raw": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-toc": "^8.0.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-plugin-dts": "^1.6.6",
    "vue": "^3.2.41",
    "vue-tsc": "^1.0.9"
  }
}
