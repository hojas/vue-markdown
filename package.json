{
  "name": "vue-md-render",
  "version": "0.1.0",
  "packageManager": "pnpm@7.13.3",
  "license": "MIT",
  "keywords": [
    "remark",
    "unified",
    "markdown",
    "commonmark",
    "gfm",
    "ast",
    "vue",
    "vue-component",
    "component"
  ],
  "repository": "hojas/vue-md-render",
  "bugs": "https://github.com/hojas/vue-md-render/issues",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/vue-md-render.d.ts",
  "main": "./dist/vue-md-render.umd.cjs",
  "module": "./dist/vue-md-render.js",
  "exports": {
    ".": {
      "import": "./dist/vue-md-render.js",
      "require": "./dist/vue-md-render.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite --config vite.config.ts example",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "rehype-stringify": "^9.0.3",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/node": "^18.8.3",
    "@vitejs/plugin-vue": "^3.1.2",
    "github-markdown-css": "^5.1.0",
    "prettier": "^2.7.1",
    "prismjs": "^1.29.0",
    "rehype-raw": "^6.1.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.7",
    "vite-plugin-dts": "^1.6.5",
    "vue": "^3.2.40",
    "vue-tsc": "^1.0.3"
  }
}
